<Window x:Class="_3.csapt_projekt.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_3.csapt_projekt"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Window.Resources>
        <ImageBrush x:Key="MainBG" ImageSource="/Resources/MainBG.png"/>
    </Window.Resources>

    <Grid RenderTransformOrigin="0.368,0.533" Background="{StaticResource MainBG}">
        <Grid Height="50" Width="Auto" Background="DodgerBlue" Margin="0,0,0,384">
            <TextBlock Text=" "/>
        </Grid>
        <Image HorizontalAlignment="Left" Margin="478,266,0,0" VerticalAlignment="Top" Style="{StaticResource Computer}"/>
        <Image HorizontalAlignment="Center" Margin="0,354,0,0" VerticalAlignment="Top" Style="{StaticResource Laptop}"/>
        <Image HorizontalAlignment="Left" Margin="176,160,0,0" VerticalAlignment="Top" Style="{StaticResource server}"/>
        <Image HorizontalAlignment="Left" Margin="275,324,0,0" VerticalAlignment="Top" Style="{StaticResource Router}"/>
        <Image HorizontalAlignment="Left" Margin="642,135,0,0" VerticalAlignment="Top" Style="{StaticResource Mobile}"/>
    </Grid>

</Window>
