<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style x:Key="Computer" TargetType="Image">
        <Setter Property="Source" Value="/Resources/Pc.png"/>
        <Setter Property="Width" Value="50"/>
        <Setter Property="Height" Value="50"/>
    </Style>
    <Style x:Key="Laptop" TargetType="Image">
        <Setter Property="Source" Value="/Resources/Laptop.png"/>
        <Setter Property="Width" Value="80"/>
        <Setter Property="Height" Value="80"/>
    </Style>
    <Style x:Key="server" TargetType="Image">
        <Setter Property="Source" Value="/Resources/server.png"/>
        <Setter Property="Width" Value="70"/>
        <Setter Property="Height" Value="70"/>
    </Style>
    <Style x:Key="Router" TargetType="Image">
        <Setter Property="Source" Value="/Resources/Router.png"/>
        <Setter Property="Width" Value="60"/>
        <Setter Property="Height" Value="60"/>
    </Style>
    <Style x:Key="Mobile" TargetType="Image">
        <Setter Property="Source" Value="/Resources/Mobile.png"/>
        <Setter Property="Width" Value="60"/>
        <Setter Property="Height" Value="60"/>
    </Style>
</ResourceDictionary>