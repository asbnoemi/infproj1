<UserControl x:Class="_3.csapt_projekt.maps.Map1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:_3.csapt_projekt.maps"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <UserControl.Resources>
        <ImageBrush x:Key="MainBG" ImageSource="/Resources/MainBG.png"/>
    </UserControl.Resources>
    <Grid>
        <Grid Height="50" Background="DarkBlue" Margin="0,0,0,400">
            <Label Content="Label" HorizontalAlignment="Left" Height="177" Margin="602,-177,0,0" VerticalAlignment="Top" Width="9"/>

        </Grid>
        <Grid Height="400" Background="{StaticResource MainBG}" Margin="0,55,0,-5">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="627*"/>
                <ColumnDefinition Width="173*"/>
            </Grid.ColumnDefinitions>
            <Button HorizontalAlignment="Left" Margin="387,233,0,0" VerticalAlignment="Top" Style="{StaticResource Computer}" Click="Question_Click"/>
            <Button HorizontalAlignment="Left" Margin="236,0,0,0" VerticalAlignment="Center" Style="{StaticResource Mobile}" Click="Question_Click"/>
            <Button HorizontalAlignment="Left" Margin="460,84,0,0" VerticalAlignment="Top" Style="{StaticResource server}" Click="Question_Click"/>
            <Button HorizontalAlignment="Left" Margin="362,314,0,0" VerticalAlignment="Top" Style="{StaticResource Hacker}"/>
        </Grid>
    </Grid>
</UserControl>
