<UserControl x:Class="_3.csapt_projekt.maps.Map1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:_3.csapt_projekt.maps"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <UserControl.Resources>
        <ImageBrush x:Key="MainBG" ImageSource="/Resources/MainBG.png"/>
    </UserControl.Resources>
    <Viewbox Stretch="Uniform">
        <Grid>

            <Grid >


                <Button x:Name="secondrow2" HorizontalAlignment="Left" Margin="544,93,0,0" VerticalAlignment="Top" Style="{StaticResource Computer}" Click="Question_Click4" IsEnabled="False"/>
                <Button x:Name="start" HorizontalAlignment="Left" Margin="387,234,0,0" VerticalAlignment="Top" Style="{StaticResource Computer}" Click="Question_Click1"/>
                <Button x:Name="firstrow1" HorizontalAlignment="Left" Margin="236,0,0,0" VerticalAlignment="Center" Style="{StaticResource Mobile}" Click="Question_Click3" IsEnabled="False"/>
                <Button x:Name="firstrow2" HorizontalAlignment="Left" Margin="449,160,0,0" VerticalAlignment="Top" Style="{StaticResource server}" Click="Question_Click2" IsEnabled="False"/>
                <Button HorizontalAlignment="Left" Margin="362,300,0,0" VerticalAlignment="Top" Style="{StaticResource Hacker}"/>
                <Button x:Name="secondrow1" HorizontalAlignment="Left" Margin="110,73,0,0" VerticalAlignment="Top" Style="{StaticResource server}" Click="Question_Click5" IsEnabled="False"/>
                <Button x:Name="thirdrow1" HorizontalAlignment="Left" Margin="449,28,0,0" VerticalAlignment="Top" Style="{StaticResource Mobile}" Click="Question_Click5" IsEnabled="False"/>
                <Button x:Name="thirdrow2" HorizontalAlignment="Left" Margin="607,28,0,0" VerticalAlignment="Top" Style="{StaticResource Mobile}" Click="Question_Click5" IsEnabled="False"/>
                <Button Content="Menu" HorizontalAlignment="Left" Height="28" Margin="690,350,0,0" VerticalAlignment="Top" Width="81" BorderBrush="White" Foreground="White" FontSize="18" FontWeight="Bold" FontFamily="Impact" Click="GoStartMenu_Click">
                    <Button.Background>
                        <SolidColorBrush Color="Black" Opacity="0.2"/>
                    </Button.Background>
                </Button>
                <Rectangle x:Name="e2Rec" HorizontalAlignment="Left" Height="46" Margin="437,203,0,0" Stroke="#FFF70000" VerticalAlignment="Top" Width="2" RenderTransformOrigin="0.5,0.5" Fill="#FFF70000">
                    <Rectangle.RenderTransform>
                        <TransformGroup>
                            <ScaleTransform ScaleX="-1"/>
                            <SkewTransform/>
                            <RotateTransform Angle="57.902"/>
                            <TranslateTransform/>
                        </TransformGroup>
                    </Rectangle.RenderTransform>
                </Rectangle>
                <Rectangle x:Name="e1Rec" HorizontalAlignment="Left" Height="93" Margin="337,175,0,0" Stroke="#FFF70000" VerticalAlignment="Top" Width="2" RenderTransformOrigin="0.5,0.5" Fill="#FFF70000">
                    <Rectangle.RenderTransform>
                        <TransformGroup>
                            <ScaleTransform ScaleX="-1"/>
                            <SkewTransform/>
                            <RotateTransform Angle="-68.151"/>
                            <TranslateTransform/>
                        </TransformGroup>
                    </Rectangle.RenderTransform>
                </Rectangle>
                <Rectangle x:Name="m2Rec" HorizontalAlignment="Left" Height="46" Margin="531,136,0,0" Stroke="#FFF70000" VerticalAlignment="Top" Width="2" RenderTransformOrigin="0.5,0.5" Fill="#FFF70000">
                    <Rectangle.RenderTransform>
                        <TransformGroup>
                            <ScaleTransform ScaleX="-1"/>
                            <SkewTransform/>
                            <RotateTransform Angle="45.143"/>
                            <TranslateTransform/>
                        </TransformGroup>
                    </Rectangle.RenderTransform>
                </Rectangle>
                <Rectangle x:Name="h2Rec" HorizontalAlignment="Left" Height="23" Margin="608,81,0,0" Stroke="#FFF70000" VerticalAlignment="Top" Width="2" RenderTransformOrigin="0.5,0.5" Fill="#FFF70000">
                    <Rectangle.RenderTransform>
                        <TransformGroup>
                            <ScaleTransform ScaleX="-1"/>
                            <SkewTransform/>
                            <RotateTransform Angle="57.902"/>
                            <TranslateTransform/>
                        </TransformGroup>
                    </Rectangle.RenderTransform>
                </Rectangle>
                <Rectangle x:Name="h1Rec" HorizontalAlignment="Left" Height="46" Margin="519,63,0,0" Stroke="#FFF70000" VerticalAlignment="Top" Width="2" RenderTransformOrigin="0.5,0.5" Fill="#FFF70000">
                    <Rectangle.RenderTransform>
                        <TransformGroup>
                            <ScaleTransform ScaleX="-1"/>
                            <SkewTransform/>
                            <RotateTransform Angle="-58.363"/>
                            <TranslateTransform/>
                        </TransformGroup>
                    </Rectangle.RenderTransform>
                </Rectangle>
            </Grid>
            <Rectangle x:Name="m1Rec" HorizontalAlignment="Left" Height="80" Margin="211,119,0,0" Stroke="#FFF70000" VerticalAlignment="Top" Width="2" RenderTransformOrigin="0.5,0.5" Fill="#FFF70000">
                <Rectangle.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform ScaleX="-1"/>
                        <SkewTransform/>
                        <RotateTransform Angle="-52.31"/>
                        <TranslateTransform/>
                    </TransformGroup>
                </Rectangle.RenderTransform>
            </Rectangle>
        </Grid>
    </Viewbox>
</UserControl>
