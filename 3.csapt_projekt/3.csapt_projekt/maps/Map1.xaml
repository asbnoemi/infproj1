<UserControl x:Class="_3.csapt_projekt.maps.Map1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:_3.csapt_projekt.maps"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <UserControl.Resources>
        <ImageBrush x:Key="MainBG" ImageSource="/Resources/MainBG.png"/>
    </UserControl.Resources>
    <Grid Background="{StaticResource MainBG}">
        <Button HorizontalAlignment="Left" Margin="387,253,0,0" VerticalAlignment="Top" Style="{StaticResource Computer}" Click="Question_Click"/>
        <Button HorizontalAlignment="Left" Margin="217,278,0,0" VerticalAlignment="Top" Style="{StaticResource Mobile}" Click="Question_Click"/>
        <Button HorizontalAlignment="Left" Margin="490,0,0,0" VerticalAlignment="Center" Style="{StaticResource Router}" Click="Question_Click"/>
        <Button HorizontalAlignment="Left" Margin="362,350,0,0" VerticalAlignment="Top" Style="{StaticResource Hacker}"/>
    </Grid>
</UserControl>
