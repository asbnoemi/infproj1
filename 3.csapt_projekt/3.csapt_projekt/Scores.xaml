<UserControl x:Class="_3.csapt_projekt.Scores"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:_3.csapt_projekt"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             Background="#FF274CB2">
    <Viewbox Stretch="Uniform">
    <Grid Background="#FF274CB2">
        <Grid Height="450" Width="800">
            <TextBox x:Name="first" HorizontalAlignment="Center" Height="141" Margin="0,181,0,0" Text="1" VerticalAlignment="Top" Width="144" FontSize="50" FontFamily="Microsoft New Tai Lue" TextAlignment="Center" FontWeight="Bold" TextWrapping="Wrap" BorderThickness="1,1,1,1" Background="White" BorderBrush="Black" IsReadOnly="True"/>
            <TextBox x:Name="second" HorizontalAlignment="Left" Height="105" Margin="197,217,0,0" Text="2" VerticalAlignment="Top" Width="131" FontSize="40" FontFamily="Microsoft New Tai Lue" TextAlignment="Center" FontWeight="Bold" TextWrapping="Wrap" BorderThickness="1,1,1,1" Background="White" BorderBrush="Black" IsReadOnly="True"/>
            <TextBox x:Name="third" HorizontalAlignment="Right" Height="86" Margin="0,236,182,0" Text="3" VerticalAlignment="Top" Width="146" FontSize="40" FontFamily="Microsoft New Tai Lue" TextAlignment="Center" FontWeight="Bold" TextWrapping="Wrap" BorderThickness="1,1,1,1" Background="White" BorderBrush="Black" IsReadOnly="True"/>
            <ComboBox x:Name="ComboBox" HorizontalAlignment="Center" Height="23" Margin="0,338,0,0" VerticalAlignment="Top" Width="686" BorderBrush="White" Background="White" Foreground="#FF080808"/>
            <Label Content="Highscores" HorizontalAlignment="Center" Height="88" Margin="0,25,0,0" VerticalAlignment="Top" Width="294" Foreground="White" FontSize="60" FontFamily="Impact"/>
            <TextBox HorizontalAlignment="Center" Height="34" Margin="0,142,0,0" TextWrapping="Wrap" Text="Highest" VerticalAlignment="Top" Width="106" Foreground="White" Background="{x:Null}" BorderBrush="{x:Null}" FontFamily="Impact" FontSize="25" TextAlignment="Center" IsReadOnly="True"/>
            <TextBox HorizontalAlignment="Left" Height="34" Margin="210,178,0,0" TextWrapping="Wrap" Text="2nd" VerticalAlignment="Top" Width="106" Foreground="White" Background="{x:Null}" BorderBrush="{x:Null}" FontFamily="Impact" FontSize="25" TextAlignment="Center" IsReadOnly="True"/>
            <TextBox HorizontalAlignment="Right" Height="34" Margin="0,202,202,0" TextWrapping="Wrap" Text="3rd" VerticalAlignment="Top" Width="106" Foreground="White" Background="{x:Null}" BorderBrush="{x:Null}" FontFamily="Impact" FontSize="25" TextAlignment="Center" IsReadOnly="True"/>
            <Button Content="Back" HorizontalAlignment="Left" Height="32" Margin="691,408,0,0" VerticalAlignment="Top" Width="99" FontFamily="Impact" FontSize="20" Foreground="White" BorderBrush="{x:Null}" Background="{x:Null}" Click="GoStartMenu_Click"/>
        </Grid>
    </Grid>
        </Viewbox>
</UserControl>
